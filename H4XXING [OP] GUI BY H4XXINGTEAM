local Button = Tab:CreateButton({
   Name = "GIVE CLASSIC SWORDüó°",
   Callback = function()
   -- The function that takes place when the button is pressed
local swordName = "ClassicSword"
local swordStorage = game.ServerStorage:FindFirstChild(swordName)
local part = script.Parent

local function onTouch(hit)
    local character = hit.Parent
    local player = game.Players:GetPlayerFromCharacter(character)
    if player and character:FindFirstChild("Humanoid") then
        local backpack = player:FindFirstChild("Backpack")
        if backpack and not backpack:FindFirstChild(swordName) and not character:FindFirstChild(swordName) then
            local swordClone = swordStorage:Clone()
            swordClone.Parent = backpack
            print("Sword given to", player.Name)
        end
    end
end

part.Touched:Connect(onTouch)
   end,
})

