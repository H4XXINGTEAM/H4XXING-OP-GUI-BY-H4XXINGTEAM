local Button = Tab:CreateButton({
   Name = "GOOFY V1(R6)üë§",
   Callback = function()
   -- The function that takes place when the button is pressed
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local runService = game:GetService("RunService")

-- Wait for all required body parts
local torso = character:WaitForChild("Torso")
local leftArm = character:WaitForChild("Left Arm")
local rightArm = character:WaitForChild("Right Arm")
local rootPart = character:WaitForChild("HumanoidRootPart")

-- Stop animations
local humanoid = character:WaitForChild("Humanoid")
humanoid:LoadAnimation(Instance.new("Animation")) -- Stop all animations

-- Anchor everything to control pose
torso.Anchored = true
leftArm.Anchored = true
rightArm.Anchored = true
rootPart.Anchored = true

-- T-pose arm positions
leftArm.CFrame = torso.CFrame * CFrame.new(-1.5, 0.5, 0)
rightArm.CFrame = torso.CFrame * CFrame.new(1.5, 0.5, 0)

-- Spin both arms continuously
local leftAngle = 0
local rightAngle = 0

runService.RenderStepped:Connect(function(dt)
	leftAngle = leftAngle + dt * 10
	rightAngle = rightAngle + dt * 10

	leftArm.CFrame = torso.CFrame * CFrame.new(-1.5, 0.5, 0) * CFrame.Angles(0, 0, leftAngle)
	rightArm.CFrame = torso.CFrame * CFrame.new(1.5, 0.5, 0) * CFrame.Angles(0, 0, rightAngle)
end)
   end,
})

local Button = Tab:CreateButton({
   Name = "UNDO GOOFY V1(R6)üë§",
   Callback = function()
   -- The function that takes place when the button is pressed
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")

-- Unanchor all body parts
for _, partName in pairs({"Torso", "Left Arm", "Right Arm", "HumanoidRootPart"}) do
	local part = character:FindFirstChild(partName)
	if part then
		part.Anchored = false
	end
end

-- Optional: Play idle animation again (automatically handled by Roblox after unanchor)
humanoid:LoadAnimation(Instance.new("Animation")) -- Triggers default animations to resume
   end,
})
