local Button = Tab:CreateButton({
   Name = "WHITE CHARACTER FLAMEü§çüî•",
   Callback = function()
   -- The function that takes place when the button is pressed
-- üî• Only-You White Fire Script (Visible to Others)
-- Place this in ServerScriptService

local Players = game:GetService("Players")

-- Replace with your Roblox UserId
local YOUR_USER_ID = 8317715190 -- ‚¨ÖÔ∏è Change this to your real UserId

local function igniteCharacter(character)
	for _, part in pairs(character:GetChildren()) do
		if part:IsA("BasePart") and not part:FindFirstChild("WhiteFire") then
			local fire = Instance.new("Fire")
			fire.Name = "WhiteFire"
			fire.Size = 25
			fire.Heat = 15
			fire.Color = Color3.new(1, 1, 1)
			fire.SecondaryColor = Color3.new(0.9, 0.9, 0.9)
			fire.Parent = part
		end
	end
end

-- Trigger fire effect only for your character
Players.PlayerAdded:Connect(function(player)
	if player.UserId == YOUR_USER_ID then
		player.CharacterAdded:Connect(function(character)
			wait(1)
			igniteCharacter(character)
		end)
	end
end)

-- Optional: for live test, ignite instantly if you're already in the game
for _, player in ipairs(Players:GetPlayers()) do
	if player.UserId == YOUR_USER_ID and player.Character then
		igniteCharacter(player.Character)
	end
end
   end,
})

local Button = Tab:CreateButton({
   Name = "Undo WHITE FLAME CHARACTERü§çüî•",
   Callback = function()
   -- The function that takes place when the button is pressed
-- üßØ Undo White Flame Character Effect (Server-Side)
-- Put this script in ServerScriptService

local Players = game:GetService("Players")

-- Replace with your real Roblox UserId
local YOUR_USER_ID = 8317715190  -- ‚Üê Change this!

local function undoWhiteFlameEffect(character)
	for _, part in pairs(character:GetDescendants()) do
		if part:IsA("BasePart") then
			-- Remove fire effect
			local fire = part:FindFirstChild("WhiteFire")
			if fire then
				fire:Destroy()
			end

			-- Reset material and color
			part.Material = Enum.Material.Plastic
			part.Color = Color3.new(0.8, 0.8, 0.8) -- light gray
		end
	end

	-- Remove any Highlight (optional)
	local highlight = character:FindFirstChildOfClass("Highlight")
	if highlight then
		highlight:Destroy()
	end
end

Players.PlayerAdded:Connect(function(player)
	if player.UserId == YOUR_USER_ID then
		player.CharacterAdded:Connect(function(character)
			wait(1)
			undoWhiteFlameEffect(character)
		end)
	end
end)

-- Optional: apply immediately if player is already in-game
for _, player in ipairs(Players:GetPlayers()) do
	if player.UserId == YOUR_USER_ID and player.Character then
		undoWhiteFlameEffect(player.Character)
	end
end
   end,
})

